{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-extra-parens": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-empty-function": 2,
    "camelcase": 2,
    "no-trailing-spaces": 2,
    "object-curly-spacing": 2,
    "no-whitespace-before-property": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ]
  },
  "parserOptions": {
   "ecmaVersion": 6
 }
}
